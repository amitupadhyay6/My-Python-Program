    "from sklearn import datasets\n",
    "\n",
    "iris_data = datasets.load_iris()\n",
    "x = iris_data.data[:,:2]\n",
    "\n",
    "xcount = len(x)\n",
    "xmin = x[:,0].min() - .5\n",
    "xmax = x[:,0].max() + .5\n",
    "xmean = x[:,0].mean()\n",
    "\n",
    "print(xcount, xmin, xmax, xmean)"
